{
  "name": "material-components",
  "version": "0.2.5",
  "description": "Stateless UI components for react that follow material design",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src app test",
    "dev": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"eslint src app test && mocha --compilers js:babel/register --recursive --reporter dot\" src app test' 'node-sass --watch src/index.scss lib/index.css' 'webpack-dev-server'",
    "prebuild": "npm run lint && rimraf lib && rimraf build",
    "build": "babel src --out-dir lib && node-sass src/index.scss lib/index.css && webpack --config webpack.config.js",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/material-components.git"
  },
  "keywords": [
    "appbar",
    "button",
    "calendar",
    "cerebral",
    "checkbox",
    "component",
    "datepicker",
    "dialog",
    "form",
    "grid",
    "input",
    "material design",
    "material",
    "material-components",
    "menu",
    "react",
    "react-component",
    "select",
    "sidenav",
    "table"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/material-components/issues"
  },
  "homepage": "https://github.com/garth/material-components#readme",
  "dependencies": {
    "exenv": "^1.2.0",
    "node-waves": "^0.7.4"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^5.4.0",
    "baobab": "^2.1.0",
    "bootstrap-sass": "^3.3.5",
    "cerebral": "^0.25.1",
    "cerebral-baobab": "^0.4.2",
    "cerebral-react": "^0.6.0",
    "cerebral-router": "^0.7.1",
    "chai": "^3.4.1",
    "classnames": "^2.2.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.1",
    "eslint-plugin-react": "^3.10.0",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "parallelshell": "^2.0.0",
    "path-to-regexp": "^1.2.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-tap-event-plugin": "^0.2.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "watch": "^0.16.0",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.0",
    "moment": "^2.10.6",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-tap-event-plugin": "^0.2.1"
  }
}
